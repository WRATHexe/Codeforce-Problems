#include<iostream>
using namespace std;
int main(){

    int N,a,i,e=0,o=0,p=0,n=0;
    cin>>N;

    for(i=1;i<=N;i++)
        {
            cin>>a;
            if(a%2==0)
            {

                e++;
            }
            else
            {

                 o++;
            }
            if(a>0)
            {

                p++;
            }
            else if(a<0)
            {

                 n++;
            }
}
cout<<"Even: "<<e<<endl;
cout<<"Odd: "<<o<<endl;
cout<<"Positive: "<<p<<endl;
cout<<"Negative: "<<n<<endl;
}
