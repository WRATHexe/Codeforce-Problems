#include<iostream>
#include<string>
using namespace std;
int main(){

 string a,b;
 int n,c=0;
 cin>>n;
 b="314159265358979323846264338327";
for(int i=0;i<n;i++)
   {

       cin>>a;
       for(int j=0;j<a.size();j++)
       {
            if(a[j]!=b[j])
            {
                break;
            }
            else
                c++;
       }
      cout<<c<<endl;
      c=0;
   }

}
