#include<iostream>
#include<string.h>
using namespace std;
int main(){

int n,a=0,b=0,space=0,m=0;
cin>>n;
string s;
cin>>s;
for(int i=0;i<n;i++)
    {
       if(s[i]=='A')
        a++;
       else
        b++;
    }
    if(a>b)
        cout<<"Anton"<<endl;
    if(a<b)
        cout<<"Danik"<<endl;
    else
        cout<<"Friendship"<<endl;
}
